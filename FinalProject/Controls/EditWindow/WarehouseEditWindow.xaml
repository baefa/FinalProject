<Window x:Class="FinalProject.Controls.EditWindow.WarehouseEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:FinalProject.Controls.EditWindow"
        mc:Ignorable="d"
        Title="WarehouseEditWindow" Height="250" Width="250">
    <Grid>
        <TextBlock x:Name="SName" HorizontalAlignment="Left" Margin="27,24,0,0" TextWrapping="Wrap"
                   Text="Название" VerticalAlignment="Top"/>
        <TextBlock x:Name="Address" HorizontalAlignment="Left" Margin="27,56,0,0" TextWrapping="Wrap"
                   Text="Адрес" VerticalAlignment="Top" />
        <TextBox x:Name="NameInput" HorizontalAlignment="Left" Margin="102,24,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" />
        <TextBox x:Name="AddressInput" HorizontalAlignment="Left" Margin="102,55,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" />
        <Button x:Name="CreateButtonL" Content="Сохранить" HorizontalAlignment="Center" Margin="0,182,0,0"
                VerticalAlignment="Top" Click="SaveButton_Click" />
    </Grid>
</Window>
